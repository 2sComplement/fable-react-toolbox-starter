{
    "module": "amd",
    "sourceMaps": true,
    "projFile": "Starter.fsproj",
    "outDir": "out",
    "refs": {
        "Fable.Core": "fable-core/umd",
        "Fable.React": "fable-react/umd"
    },
    "scripts": {
        "prebuild": "npm install"
    },
    "targets": {
        "debug": {
            "watch": true,
            "symbols": [
